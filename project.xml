<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="FootNote" package="com.sample.footnote" version="1.0.0" company="FootNote" />
	<app main="Startup" path="Export" file="FootNote" />

	<window fullscreen="false" width="640" height="480" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="starling" />
	
	<assets path="Assets" rename="assets" />

	<architecture name="armv64" exclude="armv7" if="android" />
	<config:android target-sdk-version="33"/>

	<config:android>
		<config:android permission="android.permission.INTERNET" />
		<config:android permission="android.permission.BLUETOOTH_SCAN" />
	</config:android>

	<certificate path="my-release-key.jks" alias="1" if="android"/>
	
</project>